                                </div>
                            </div>

                            <div class="form-group">
                                @Html.LabelFor(model => model.Physical, new { @class = "col-sm-2 control-label" })
                                <div class="col-sm-8">
                                    @Html.TextBoxFor(model => model.Physical, new { @class = "form-control1", @required = "required" })
                                </div>
                                <div class="col-sm-2">
